import os;

struct Point {
        int x;
        int y;
};

int main() {
    os.println("Struct Test Start");
    
    struct Point p;
    p.x = 10;
    p.y = 20;
    os.println("p.x = " + os.str(p.x));
    
    struct Rect {
        int w;
        int h;
    } r;
    r.w = 100;
    os.println("r.w = " + os.str(r.w));
    
    // Test Array
    struct Point points[5];
    points[0].x = 123;
    os.println("points[0].x = " + os.str(points[0].x));
    
    struct Node {
        int id;
    } nodes[3];
    nodes[1].id = 999;
    os.println("nodes[1].id = " + os.str(nodes[1].id));

    return 0;
}
